<section id="menu" class="menu container mx-auto py-8">
    <h1 class="text-xl font-bold mb-8 mx-10"> All Dishes </h1>


    <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 xl:grid-cols-4 gap-x-12 gap-y-16">

        <% menus.forEach(function(dish){ %>
            <div class="w-full md:w-64">
                <img class="mx-10 h-40 mb-4 mx-auto" src="/img/<%=dish.image %>" alt="food items">
                <div class="mx-10 text-center">

                    <h2 class=" mb-4 text-lg">
                        <%= dish.name %>
                    </h2>
                    <span class="py-1 px-4 rounded-full uppercase text-xs size "><%= dish.size %></span>

                    <div class="flex items-center justify-around py-4">

                        <span class="font-bold text-lg"> Rs. <%= dish.price %></span>

                        <button data-dish="<%= JSON.stringify(dish) %>" id="add-to-cart" class="add-to-cart py-1 px-6 rounded-full flex items-center font-bold">
                                <span>+</span>
                                <span class="ml-4 ">Add</span>
                            </button>
                    </div>
                </div>

            </div>



            <% }) %>










    </div>


</section>

